<div id="page-wrapper">
  <div id="page">

  <!--Start: Branding and Menu -->
<div id="stick">
  <div class="menu">
    <div class="container">
      <div class="row">
        {% if page.header %}
          <div class="col-xs-6 col-md-4">
            {{ page.header }}
          </div>
        {% endif %}
        <!-- Navigation -->
        {% if page.menu %}
          <div class="col-xs-6 col-md-8">
            {{ page.menu }} 
          </div>
        {% endif %}
        <!--End Navigation -->
      </div>
    </div>
  </div>
</div>

      <!--- Start Tienda -->
        <div class="container-fluid tienda box">
          <div class="row">
            <div class="col-md-12">
              {{ page.tiendabanner }}
            </div>
          </div>
        </div>
      <!---End Tienda -->

      <!--- Start Messages -->
      {% if page.messages %}
        <div class="container">
          <div class="row">
              {{ page.messages }}
          </div>
        </div>
      {% endif %}
      <!---End Messages -->


      <!--- Start content -->
        <div class="container">
         <div class="row tiendaspace">
          <div class="col-md-12">
            <div class="row">
              <h2>Instituciones</h2>
              <dd>{{ drupal_view('productos_tienda', 'block_2') }}</dd>
            </div>
            <div class="row">
              <h2>Línea accesorios</h2>
              <dd>{{ drupal_view('productos_tienda', 'block_3') }}</dd>
            </div>
            <div class="row">
              <h2>Línea decoración</h2>
              <dd>{{ drupal_view('productos_tienda', 'block_4') }}</dd>
            </div>
            <div class="row">
              <h2>Línea utilitarios</h2>
              <dd>{{ drupal_view('productos_tienda', 'block_5') }}</dd>
            </div>
          </div>
         </div>
        </div>
      <!---End content -->

{#             {{ page.contenttienda }} #}



